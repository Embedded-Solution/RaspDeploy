{
  "name": "Totemhome",
  "description": "Add menu to totem",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "browser_action": {
    "default_title": "Add menu to totem"
  },
  "manifest_version": 2,
  "web_accessible_resources" : ["*.png","*.JPG"]
}
